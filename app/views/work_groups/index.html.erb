<p id="notice"><%= notice %></p>

<h1>Work Groups</h1>

<table>
  <thead>
    <tr>
      <th>Work Type</th>
      <th>Stage</th>
      <th>Task Type</th>
      <th>Role</th>
      <th>Environment</th>
      <th>Portfolio</th>
      <th>Work Group Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_groups.each do |work_group| %>
      <tr>
        <td><%= work_group.work_type_id %></td>
        <td><%= work_group.stage_id %></td>
        <td><%= work_group.task_type_id %></td>
        <td><%= work_group.role_id %></td>
        <td><%= work_group.environment_id %></td>
        <td><%= work_group.portfolio_id %></td>
        <td><%= work_group.work_group_order %></td>
        <td><%= link_to 'Show', work_group %></td>
        <td><%= link_to 'Edit', edit_work_group_path(work_group) %></td>
        <td><%= link_to 'Destroy', work_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work Group', new_work_group_path %>
